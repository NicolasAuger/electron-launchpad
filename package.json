{
  "name": "poool-launchpad",
  "version": "1.0.0",
  "productName": "Launchpad",
  "executableName": "Launchpad",
  "description": "An electron app to configure, map and use novation launchpad",
  "type": "module",
  "main": ".vite/build/main.js",
  "author": "Nicolas Auger",
  "license": "MIT",
  "scripts": {
    "start": "ELECTRON_ENABLE_LOGGING=true electron .",
    "dev": "electron-forge start",
    "rebuild": "electron-rebuild -f -w midi",
    "lint": "eslint --max-warnings=0 --cache .",
    "serve": "nodemon server.js"
  },
  "dependencies": {
    "@julusian/midi": "3.6.1",
    "body-parser": "2.2.1",
    "cors": "2.8.5",
    "express": "5.2.1",
    "morgan": "1.10.1",
    "sound-play": "1.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "7.10.2",
    "@electron-forge/maker-deb": "7.10.2",
    "@electron-forge/maker-rpm": "7.10.2",
    "@electron-forge/maker-squirrel": "7.10.2",
    "@electron-forge/maker-zip": "7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "7.10.2",
    "@electron-forge/plugin-fuses": "7.10.2",
    "@electron-forge/plugin-vite": "7.10.2",
    "@electron/fuses": "2.0.0",
    "@poool/eslint-config": "5.0.7",
    "@reforged/maker-appimage": "5.1.1",
    "@types/debug": "4.1.12",
    "@types/electron-squirrel-startup": "1.0.2",
    "@types/fs-extra": "11.0.4",
    "@types/sound-play": "1.1.3",
    "debug": "4.4.3",
    "dotenv": "17.2.3",
    "electron": "39.2.6",
    "eslint": "9.39.1",
    "nodemon": "3.1.11",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "vite": "7.2.7"
  },
  "packageManager": "yarn@4.12.0"
}
